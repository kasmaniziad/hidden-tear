<div class="" style="min-height:703px;">
    <div class="col-md-12 dayClass" ng-show="divIsBusy" style="position:absolute;width:100%;">
        <div style="position:relative;width:50%;margin:200px auto;">
            <div class="divStyle"></div>

            <img style="width:100%;" src="@Url.Content("~/")images/Vehicle.png" />
        </div>
    </div>
    <div class="row">
        @*<div class="col-md-12" style="margin-bottom:20px;" ng-hide="divIsBusy">
                <div class="row customRowStyle">
                    <div class="col-md-12">
                        <label class="labelClass"><i class="fa fa-envelope iconNewStyle">&nbsp;</i>Official Email : &nbsp;</label><label class="labelValueClass">{{userInfo.EmailId}}</label>
                    </div>
                </div>
                <div class="row customRowStyle">
                    <div class="col-md-6">
                        <label class="labelClass"><i class="fa fa-user iconNewStyle">&nbsp;</i>Manager Name : &nbsp;</label><label class="labelValueClass">{{userInfo.UserName}}</label>
                    </div>
                    <div class="col-md-6">
                        <label class="labelClass"><i class="fa fa-mobile iconNewStyle">&nbsp;</i>Manager Mobile : &nbsp;</label><label class="labelValueClass">{{userInfo.MobileNo}}</label>
                    </div>
                </div>
            </div>*@
        <div class="col-md-12 dayClass" ng-hide="divIsBusy">
            <div class="row">
                <div class="col-md-6" style="padding-right:0px;">
                    <div class="col-md-10">
                        <h2 ng-hide="divIsBusy" class="defaultcaptions aligncenter">
                            Credit Limit
                        </h2>
                        <canvas-gauge width="360" under-color="#EFEFEF" over-color="#274b90" value="60" mask="({0})%"
                                      style="color: gray;"></canvas-gauge>
                    </div>
                    <div class="col-md-2" style="padding-left:0px;padding-top:56px;">
                        <div class="row">
                            <div class="divchart"></div><label>&nbsp; Used</label>
                        </div>
                        <div class="row">
                            <div class="divchart" style="background-color:#efefef"></div> <label>&nbsp; Remaining</label>
                        </div>
                    </div>
                </div>
                <div class="col-md-offset-1 col-md-5" style="padding-right:0px;">
                    <h2 ng-hide="divIsBusy" class="defaultcaptions">
                        Sites
                    </h2>
                    <div ng-repeat="project in lstUserData" style="padding-top:10px;">
                        <div>
                            <a ng-click="redirectto('placeorderwithsite', project.SiteId)" class="primarycolor" style="font-size:larger;font-weight:bold;cursor:pointer;">
                                {{project.SiteNumber}} - {{project.SiteName}}
                            </a>
                            @*<blink>
                                <span style="color:red;position:absolute;">
                                    &nbsp;&nbsp; Order Now!
                                </span>
                            </blink>*@

                        </div>
                        <div>
                            <label>Status : Active</label>
                        </div>
                        <div>
                            <label>Last Delivered On : Oct-17-2016 </label>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <h2 ng-hide="divIsBusy" class="defaultcaptions aligncenter" style="margin-bottom:2px;margin-top:10px;font-size:24px;">
                        Recent Orders
                    </h2>
                    <table class="table table-bordered">
                        <thead>
                            <tr>
                                <th class="divCaption">Requested For</th>
                                <th class="divCaption aligncenter">Project</th>
                                <th class="divCaption aligncenter">Requested On</th>
                                <th class="divCaption aligncenter">Requested By</th>
                                <th class="divCaption aligncenter">Status</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="order in $parent.dashboardOrders">
                                <td class="aligncenter defaultpadding">
                                    {{order.RequestedDate | date}}
                                </td>
                                <td class="aligncenter defaulttextallignment defaultpadding">
                                    {{order.SiteName}}
                                </td>
                                <td class="aligncenter defaultpadding">
                                    {{order.RequestedDate | date}}
                                </td>
                                <td class="aligncenter defaultpadding">
                                    {{order.UserName}}
                                </td>
                                <td class="aligncenter defaultpadding">
                                    <label ng-class="{'greencolor': order.IsExported == true, 'orangecolor' : order.IsExported == false}">{{order.IsExported == true ? 'Delivered' : 'Pending' }}</label>
                                </td>
                            </tr>
                        </tbody>
                    </table>

                </div>
            </div>


        </div>
        <div class="aligncenter" style="padding-bottom:100px;" ng-hide="divIsBusy">
            <input class="btn btn-common buttonClass" style="height:32px !important" type="button" ng-click="redirectto('placeorder')" value="Place New Order">
            <br />
            <br />
            <label><b>For further assistance please contact +974 4021 7777 (Customer Service).</b></label>


        </div>
    </div>




</div>
