
<div class="row s4-notdlg" style="margin-top:-20px;">
    <div class="col-md-12">
        <div id="WOQOD-PageTitle">
            <h1 style="padding-bottom:10px;" ng-hide="divIsBusy">
                {{userInfo.CustomerNumber}} - {{userInfo.CustomerName}}
            </h1>
        </div>
    </div>
</div>

<style>
    .floatl {
        float: left;
    }

    .divCaption {
        background-color: #f1f1f1 !important;
        padding: 10px !important;
    }

    .dayClass {
        margin-bottom: 10px;
    }

    .custompadding {
        padding: 10px !important;
    }

    .aligncenter {
        text-align: center !important;
    }

    .alignmiddle {
        vertical-align: middle !important;
    }

    .textboxClass {
        background-color: transparent !important;
        border: 0px !important;
        width: 100%;
        height: 100%;
    }

    .buttonClass {
        margin-left: 0px !important;
        padding: 4px 50px !important;
        background-color: #274b90 !important;
        color: white !important;
        font-size: 15px !important;
        font-weight: 600 !important;
        height: 32px !important;
    }

    .orderNowButtonClass {
        background-color: #274b90 !important;
        color: white !important;
        font-size: 13px !important;
        line-height: 14px;
        width: 87px !important;
        text-align: center;
        padding-left: 7px;
        padding-top: 7px;
    }

    .changeOrderButtonClass {
        background-color: #274b90 !important;
        color: white !important;
        font-size: 13px !important;
        line-height: 14px;
        width: 100px !important;
        text-align: center;
        padding-left: 7px;
        padding-top: 7px;
    }

    progress[value]::-webkit-progress-bar {
        background-color: #eee;
        border-radius: 2px;
        box-shadow: 0 2px 5px rgba(0, 0, 0, 0.25) inset;
    }

    progress[value]::-webkit-progress-value {
        background-image: -webkit-linear-gradient(-45deg, transparent 33%, rgba(0, 0, 0, .1) 33%, rgba(0,0, 0, .1) 66%, transparent 66%), -webkit-linear-gradient(top, rgba(255, 255, 255, .25), rgba(0, 0, 0, .25)), -webkit-linear-gradient(left, #09c, #f44);
        border-radius: 2px;
        background-size: 35px 20px, 100% 100%, 100% 100%;
    }

    .divStyle {
        position: absolute;
        width: 43%;
        right: 16px;
        margin-top: 20.5%;
    }

    .progress {
        margin-bottom: 0px;
        width: 100%;
        height: 6px;
        background-color: #033399;
    }

    /*.progress {
        margin-bottom: 0px;
        position: absolute;
        width: 232px;
        height: 6px;
        margin-top: 91px;
        margin-left: 204px;
        background-color: red;
    }*/


    .progress-bar {
        background-color: white;
    }

    .shadowDiv {
        -webkit-box-shadow: 10px 8px 26px -4px rgba(209,201,209,1);
        -moz-box-shadow: 10px 8px 26px -4px rgba(209,201,209,1);
        box-shadow: 10px 8px 26px -4px rgba(209,201,209,1);
        border-radius: 4px;
    }

    .headerclass {
        color: #ffffff;
        font-weight: bold;
        padding: 5px 5px 2px 10px;
        /* padding-left: 15px; */
        font-size: 17px;
        border-bottom: 1px solid;
        border-color: #274b90;
        background-color: #274b90;
        border-radius: 5px;
    }

    .iconcolor {
        color: white !important;
    }

    .creditlimitsize {
        padding-bottom: 12px;
        font-weight: bold;
        font-size: 20px;
    }

    .labelClass {
        color: #274b90;
        font-size: 23px;
        font-weight: bold;
    }

    .labelValueClass {
        font-size: 23px;
    }

    .iconNewStyle {
        color: #009d59;
    }

    .customRowStyle {
        margin-bottom: 20px;
    }
</style>

